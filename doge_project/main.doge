#import SL;

float pi(float n) {
    float sum = 0.0;
    float sign = 1.0;
    for (float i = 0.0; i < n; i = i + 1.0) {
        sum =  sum + sign/(2.0*i+1.0);
        sign = sign * -1.0;
    }
    return 4.0*sum;
}

