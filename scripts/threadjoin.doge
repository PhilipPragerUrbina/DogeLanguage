#import SL;
#import Thread;
#import Random;
function newThread(num, id, in){
int e = 0;
int max =  RandomInt(0,num);
    for(int i = 0; i <= max; i++){
       e = e + i;
    }
    if(id == 0){
        &in = e;
    }
    return num / 2;
}
int out = 0;
for(int i =0;i < 20; i++){
thread(newThread, 10, i,&out);
}


int num = sync();


print("Synced output: " + out);